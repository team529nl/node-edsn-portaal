	<!-- ===== Type Definition: NumberSegmentType ===== -->
	<!-- ================================================================== -->
	<xsd:simpleType name="FileExchangeNotificationEnvelope_NumberSegmentType">
		<xsd:annotation>
			<xsd:documentation>
				<ccts:UniqueID>urn:nedu:edsn:data:fileexchangenotification#NumberSegmentType</ccts:UniqueID>
				<ccts:VersionID>1.4</ccts:VersionID>
				<ccts:DictionaryEntryName>NumberSegmentType</ccts:DictionaryEntryName>
