	<!-- ================================================================== -->
	<!-- ===== Type Definition: Indicator ===== -->
	<!-- ================================================================== -->
	<xsd:simpleType name="FileExchangeNotificationEnvelope_Indicator">
		<xsd:annotation>
			<xsd:documentation>
				<ccts:UniqueID>urn:nedu:edsn:data:fileexchangenotification#Indicator</ccts:UniqueID>
				<ccts:VersionID>1.4</ccts:VersionID>
				<ccts:DictionaryEntryName>Indicator</ccts:DictionaryEntryName>
