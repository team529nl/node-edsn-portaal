	<!-- ================================================================== -->
	<!-- ===== Type Definition: GLNEANCode ===== -->
	<!-- ================================================================== -->
	<xsd:simpleType name="FileExchangeNotificationEnvelope_GLNEANCode">
		<xsd:annotation>
			<xsd:documentation>
				<ccts:UniqueID>urn:nedu:edsn:data:fileexchangenotification#GLNEANCode</ccts:UniqueID>
				<ccts:VersionID>1.4</ccts:VersionID>
				<ccts:DictionaryEntryName>GLNEANCode</ccts:DictionaryEntryName>
