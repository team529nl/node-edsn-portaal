			<xsd:element name="SFTPFileName" type="ccma:FileExchangeNotificationEnvelope_FileNameType" sawsdl:modelReference="urn:nedu:edsn:lom:portaal:model:1:standard#SFTPFileDetails.SFTPFileName">
				<xsd:annotation>
					<xsd:documentation>
						<ccts:UniqueID>urn:nedu:edsn:data:fileexchangenotification#SFTPFileDetails.SFTPFileName</ccts:UniqueID>
						<ccts:VersionID>1.4</ccts:VersionID>
						<ccts:DictionaryEntryName>SFTPFileDetails.SFTPFileName</ccts:DictionaryEntryName>
						<ccts:Definition>Bestandsnaam van het (deel)bestand op de sFTP server. Max. 100 karakters (LDT).</ccts:Definition>
						<ccts:BusinessTermName>Bestandsnaam</ccts:BusinessTermName>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
