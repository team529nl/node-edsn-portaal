				<ccts:DictionaryEntryName>LocationType</ccts:DictionaryEntryName>
				<ccts:BusinessTermName>Locatie</ccts:BusinessTermName>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="1000"/>
