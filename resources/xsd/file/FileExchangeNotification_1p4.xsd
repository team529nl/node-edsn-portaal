	<xsd:simpleType name="FileExchangeNotificationEnvelope_NoteType">
		<xsd:annotation>
			<xsd:documentation>
				<ccts:UniqueID>urn:nedu:edsn:data:fileexchangenotification#NoteType</ccts:UniqueID>
				<ccts:VersionID>1.4</ccts:VersionID>
				<ccts:DictionaryEntryName>NoteType</ccts:DictionaryEntryName>
				<ccts:BusinessTermName>Toelichting</ccts:BusinessTermName>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="100"/>
			<xsd:minLength value="1"/>
